{
    "name": "start",
    "version": "0.0.1",
    "description": "A way to start web applications",
    "private": true,
    "license": "MIT",
    "author": "@davi-mbatista",
    "scripts": {
        "start": "webpack serve --config webpack/start.js",
        "build": "webpack --config webpack/build.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "7.14.3",
        "@babel/preset-env": "7.14.2",
        "autoprefixer": "10.2.5",
        "babel-loader": "8.2.2",
        "clean-webpack-plugin": "3.0.0",
        "css-loader": "5.2.6",
        "file-loader": "6.2.0",
        "html-webpack-plugin": "5.3.1",
        "mini-css-extract-plugin": "1.6.0",
        "postcss": "^8.4.31",
        "postcss-cli": "8.3.1",
        "postcss-import": "14.0.2",
        "postcss-loader": "5.3.0",
        "postcss-preset-env": "^9.2.0",
        "prettier": "2.3.0",
        "purgecss-webpack-plugin": "4.0.3",
        "size-plugin": "2.0.2",
        "style-loader": "2.0.0",
        "svelte-loader": "^3.1.9",
        "svelte-preprocess": "4.7.3",
        "svelte-routing": "^2.5.0",
        "tailwindcss": "^2.2.19",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-merge": "5.7.3"
    },
    "dependencies": {
        "buffer": "^6.0.3",
        "crypto-browserify": "^3.12.0",
        "dotenv": "^16.3.1",
        "os-browserify": "^0.3.0",
        "path-browserify": "^1.0.1",
        "stream-browserify": "^3.0.0",
        "svelte": "^3.59.2",
        "vanilla-captcha": "^1.0.9"
    },
    "babel": {
        "presets": [
            "@babel/preset-env"
        ]
    },
    "browserslist": [
        "last 3 chrome version",
        "last 3 firefox version",
        "last 3 safari version",
        "last 3 ios_saf version"
    ],
    "postcss": {
        "plugins": {
            "postcss-preset-env": {},
            "postcss-import": {},
            "tailwindcss": {}
        }
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "none",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "avoid"
    }
}
