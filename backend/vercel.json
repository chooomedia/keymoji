{
    "version": 2,
    "builds": [
        {
            "src": "api/contact.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/random.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/send-magic-link.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/verify-magic-link.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/account.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/account/update.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/test-emails.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/contact",
            "dest": "/api/contact.js",
            "methods": [
                "POST",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, X-Requested-With"
            }
        },
        {
            "src": "/api/random",
            "dest": "/api/random.js",
            "methods": [
                "GET",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, X-Requested-With"
            }
        },
        {
            "src": "/api/magic-link/send",
            "dest": "/api/send-magic-link.js",
            "methods": [
                "POST",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, X-Requested-With"
            }
        },
        {
            "src": "/api/magic-link/verify",
            "dest": "/api/verify-magic-link.js",
            "methods": [
                "POST",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, X-Requested-With"
            }
        },
        {
            "src": "/api/account",
            "dest": "/api/account.js",
            "methods": [
                "POST",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, X-Requested-With"
            }
        },
        {
            "src": "/api/account/update",
            "dest": "/api/account/update.js",
            "methods": [
                "POST",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, X-Requested-With"
            }
        },
        {
            "src": "/api/test-emails",
            "dest": "/api/test-emails.js",
            "methods": [
                "GET",
                "POST",
                "OPTIONS"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
                "Access-Control-Allow-Headers": "Content-Type, X-Requested-With"
            }
        },
        {
            "src": "/.*",
            "status": 404,
            "dest": "/404"
        }
    ],
    "github": {
        "silent": true
    }
}